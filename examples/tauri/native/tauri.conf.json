{
  "productName": "tuber",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../web/dist/",
    "devUrl": "http://localhost:8080"
  },
  "plugins": {
    "tuber": {
      "allow-tx": true,
      "allow-rx": true
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tuber",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}